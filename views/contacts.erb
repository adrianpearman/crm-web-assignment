<h1>All Contacts</h1>

<h2>
  First and Last Names
</h2>
<ul>
<% Contact.all.each do |contact| %>
  <li>
  <%= contact.first_name %>
  <%= contact.last_name %>
  </li>
<% end %>
</ul>

<h2>
  Email and Notes
</h2>
<ul>
  <% Contact.all.each do |contact| %>
  <li>
    <%= contact.email %>
    <%= contact.note  %>
  </li>
  <% end %>
</ul>

<li><a href="/contacts/new">Add a New Contact</a></li>
